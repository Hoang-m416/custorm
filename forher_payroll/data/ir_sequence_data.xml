<odoo>
    <data noupdate="1">
        <record id="seq_forher_payslip_run" model="ir.sequence">
            <field name="name">Forher Payslip Run</field>
            <field name="code">forher.payslip.run</field>
            <field name="prefix">PR%(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_forher_payslip" model="ir.sequence">
            <field name="name">Forher Payslip</field>
            <field name="code">forher.payslip</field>
            <field name="prefix">PS%(y)s%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>
