<odoo>
    <record id="view_payroll_import_wizard" model="ir.ui.view">
        <field name="name">forher.payroll.import.wizard.view</field>
        <field name="model">forher.payroll.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Nhập dữ liệu doanh số">
                <group>
                    <field name="run_id" readonly="1"/>
                    <field name="delimiter"/>
                    <field name="data_file" filename="filename"/>
                    <field name="filename" invisible="1"/>
                </group>
                <footer>
                    <button name="action_import" type="object" string="Nhập" class="btn-primary"/>
                    <button string="Hủy" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
